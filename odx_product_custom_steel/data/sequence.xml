<odoo>
    <data noupdate="1">

        <record id="stock_lock_custom_serial" model="ir.sequence">
            <field name="name">Custom Serial Number Lot</field>
            <field name="code">stock.lot.serial.custom</field>
            <field name="prefix">CS</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="stock_lot_custom_baby_coil" model="ir.sequence">
            <field name="name">Baby Lot Serial Number </field>
            <field name="code">stock.lot.serial.baby.coil</field>
            <field name="prefix">CP1</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="purchase_custom_serial" model="ir.sequence">
            <field name="name">Purchase Order Custom Serial</field>
            <field name="code">purchase.serial.custom</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.order</field>
            <field name="prefix">RFQ</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>