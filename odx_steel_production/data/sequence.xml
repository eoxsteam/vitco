<odoo>
    <data>
        <record id="steel_production_custom_sequence" model="ir.sequence">
            <field name="name">Steel Production Sequence</field>
            <field name="code">steel.production.sequence</field>
            <field name="prefix">PRO/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="job_order_custom_sequence" model="ir.sequence">
            <field name="name">Job Order Sequence</field>
            <field name="code">job.order.sequence</field>
            <field name="prefix">JOB/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>